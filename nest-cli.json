{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/graphql-fed-poc/src",
  "monorepo": true,
  "root": "apps/graphql-fed-poc",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/graphql-fed-poc/tsconfig.app.json"
  },
  "projects": {
    "graphql-fed-poc": {
      "type": "application",
      "root": "apps/graphql-fed-poc",
      "entryFile": "main",
      "sourceRoot": "apps/graphql-fed-poc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql-fed-poc/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "apps/posts",
      "entryFile": "main",
      "sourceRoot": "apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    }
  }
}